  "scripts": {
    "start": "npm run dev:1 && npm run dev:2 && npm run dev:3 && npm run dev:ico",
    "ico": "tsc && node app/server/ico/ico.js",
    "dev": "set NODE_ENV=development && nodemon --config \"./nodemon.json\"/",
    "dev:1": "set HTTP_PORT=3001 & set P2P_PORT=5001 && start npm run dev",
    "dev:2": "set HTTP_PORT=3002 & set P2P_PORT=5002 & set PEERS=ws://localhost:5001 && start npm run dev",
    "dev:3": "set HTTP_PORT=3003 & set P2P_PORT=5003 & set PEERS=ws://localhost:5001,ws://localhost:5002 && start npm run dev",
    "dev:ico": "set HTTP_PORT=3000 && set P2P_PORT=5000 && set PEERS=ws://localhost:5001,ws://localhost:5002,ws://localhost:5003 && start npm run ico",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "count:lines": "git ls-files | xargs wc -l"
  },
